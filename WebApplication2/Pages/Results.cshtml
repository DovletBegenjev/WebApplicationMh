    <body>
    <h2>Entered Data</h2>

@if (Model.FormDataList.Count == 0)
{
    <p>No data submitted yet.</p>
}
else
{
    <table border="1">
        <thead>
        <tr>
            <th>ID</th>
            <th>Prod Area ID</th>
            <th>Date</th>
            <th>DayInMonth</th>
            <th>SPPFluid</th>
            <th>SPPOil</th>
            <th>Corr Fact</th>
            <th>Avg BSW</th>
            <th>Avg BSW SPP</th>
            <th>Vol Red Fact</th>
            <th>SPP Fl Temp</th>
            <th>Oil Dens</th>
            <th>Oil Del TNT</th>
            <th>Mth Oil IPR</th>
            <th>Daily Gas IPR</th>
            <th>Avail For Lifting</th>
            <th>Lifted Today</th>
            <th>Lift Port</th>
            <th>Flared Gas</th>
